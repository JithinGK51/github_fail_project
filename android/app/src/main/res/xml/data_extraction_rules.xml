<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Allow cloud backup for non-sensitive data -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="github_token.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer for non-sensitive data -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="github_token.xml"/>
    </device-transfer>
</data-extraction-rules>
